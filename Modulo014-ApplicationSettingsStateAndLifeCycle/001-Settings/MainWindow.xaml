<Window x:Class="_001_Settings.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:p="clr-namespace:_001_Settings.Properties"
        Title="MainWindow" Height="350" Width="525" Loaded="Window_Loaded" Closing="Window_Closing">
    <StackPanel>
        <Label Name="MailCorporativo"></Label>
        <TextBox Name="Mail" Text="{Binding Source={x:Static p:Settings.Default}, Path=eMail}"></TextBox>
    </StackPanel>
</Window>
