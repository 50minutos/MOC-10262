<Window x:Class="_002_Canvas.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Canvas>
        <Line Canvas.Left="10" Canvas.Top="10" X1="10" Y1="10" X2="100" Y2="100" Stroke="Black"></Line>

        <Rectangle Canvas.Left="200" Canvas.Top="200" Height="40" Width="100" Fill="Black"></Rectangle>

        <Ellipse Canvas.Left="200" Canvas.Top="200" Height="40" Width="100" Fill="White"></Ellipse>
    </Canvas>
</Window>
