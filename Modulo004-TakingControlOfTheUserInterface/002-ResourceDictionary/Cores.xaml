<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <LinearGradientBrush x:Key="degrade">
        <LinearGradientBrush.GradientStops>
            <GradientStop Offset="0.00" Color="Black" />
            <GradientStop Offset="1.00" Color="White" />
        </LinearGradientBrush.GradientStops>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="yellowBrush" Color="Yellow"></SolidColorBrush>
</ResourceDictionary>
